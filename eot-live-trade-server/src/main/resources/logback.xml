<?xml version="1.0" encoding="UTF-8"?>
<configuration>

    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger{5} - %msg%n</pattern>
        </encoder>
    </appender>

    <logger name="log4j.logger.com.impetus" level="OFF" additivity="false">
            <appender-ref ref="STDOUT" />
 
    </logger>
    
        <logger name="log4j.logger.de.wieczorek.eot" level="INFO" additivity="false">
            <appender-ref ref="STDOUT" />
 
    </logger>

    <root level="INFO">
        <appender-ref ref="STDOUT" />
    </root>
</configuration>